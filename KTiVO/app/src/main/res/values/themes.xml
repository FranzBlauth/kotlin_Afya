<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.KTiVO" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ktivobase</item>
        <item name="colorPrimaryVariant">@color/ktivobase</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/ktivobase</item>
        <item name="colorSecondaryVariant">@color/ktivobase</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#00AEDF</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.KTiVO.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.KTiVO.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.KTiVO.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>